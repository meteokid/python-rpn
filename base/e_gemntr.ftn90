#include "model_macros_f.h"
!
      subroutine gemntr
      implicit none
!
!author   
!
!revision
! v2_21 - M. Desgagne       - reshaped version
! v3_00 - Desgagne & Lee    - Lam configuration
! v3_11 - M. Desgagne       - change to exdb and exfin
! v3_21 - Lee V.            - Warning if geophy.bin not written
! v3_30 - Desgagne/Lee      - modifications for new LAM I/O piloting
! v4_03 - M. Desgagne       - ISST
! v4_05 - M. Desgagne       - Add timing information (gemtim3)
!
!implicits
#include "pilot.cdk"
#include "version.cdk"
#include "itf_phy_buses.cdk"
#include "path.cdk"
#include "lun.cdk"
!
! ---------------------------------------------------------------------
!
!     ================
      entry gemclimntr
!     ================
!
      call e_initntr

      call e_gemnml
!
      if (Pil_3df_L) then
!
         call e_grid
!
         call e_dateo
!
         call gemtim3 (6)
!
         call e_dynfld
!
         call e_nml2bin 
!
      endif
!
      call e_stopntr ()
!
! ---------------------------------------------------------------------
!
      stop
      end
