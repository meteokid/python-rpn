!**s/r set_geof - initialization of the VMM commons for geophysical fields
!
#define SPY_VMM_CREATE spy_vmm_create
#include "model_macros_f.h"
!
      subroutine set_geof
!
      implicit none
!
!author
!     Gravel/Roch - rpn - august 1993
!
!revision
! v2_00 - Desgagne/Lee      - initial MPI version (from setgeof v1_03)
! v2_20 - Lee V.            - eliminated geophysical VMM variables
! v2_21 - J. P. Toviessi    - rename some model output variables
! v3_01 - Desgagne M.       - remove XLA and XLO
! v3_11 - Gravel S.         - add field for varying topography
! v4_06 - Lepine M.         - VMM replacement with GMM
!
!object
!	This subroutine initializes the commons containing the
!	keys used by the Virtual Memory Manager to identify the
!	geophysical fields
!	
!arguments
!	none
!
!implicits
#include "gmm.hf"
#include "var_gmm.cdk"
#include "glb_ld.cdk"
#include "lun.cdk"
#include "p_geof.cdk"
#include "vtopo.cdk"
!
!modules
!*
!
!     ---------------------------------------------------------------
!
      if (Lun_out.gt.0) write(Lun_out,1000)
!
      gmmk_fis0_s  = 'FIS0'
      gmmk_fis1_s = 'FIS1'
      gmmk_dfis_s = 'DFIS'
!
      nullify(fis0, fis1,dfis)
      gmmstat = gmm_create(gmmk_fis0_s,fis0,meta2d,GMM_FLAG_RSTR+GMM_FLAG_INAN)
      if (GMM_IS_ERROR(gmmstat)) print *,'set_geof ERROR at gmm_create(fis0)'

      if ( Vtopo_start .ge. 0 ) then
         gmmstat = gmm_create(gmmk_fis1_s,fis1,meta2d,GMM_FLAG_RSTR+GMM_FLAG_INAN)
         if (GMM_IS_ERROR(gmmstat)) print *,'set_geof ERROR at gmm_create(fis1)'
         gmmstat = gmm_create(gmmk_dfis_s,dfis,meta2d,GMM_FLAG_RSTR+GMM_FLAG_INAN)
         if (GMM_IS_ERROR(gmmstat)) print *,'set_geof ERROR at gmm_create(dfis)'
      endif
!
 1000 format( &
      /,'INITIALIZATION OF GEOPHYSICAL FIELD COMDECKS (S/R SET_GEOF)', &
      /,'===========================================================')
!
!     ---------------------------------------------------------------
!
      return
      end
