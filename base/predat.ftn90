!*s/r predat6 - Performs preprocessing of the data: part1
!
#include "model_macros_f.h"
!
      subroutine predat6 (F_u, F_v, F_t, F_zd, F_w, F_s, &
                          Mminx,Mmaxx,Mminy,Mmaxy, Gnk)
      implicit none
#include <arch_specific.hf>
!
      integer Mminx,Mmaxx,Mminy,Mmaxy,Gnk
      real F_u(Mminx:Mmaxx,Mminy:Mmaxy,Gnk), F_v (Mminx:Mmaxx,Mminy:Mmaxy,Gnk), &
           F_t(Mminx:Mmaxx,Mminy:Mmaxy,Gnk+1), F_zd(Mminx:Mmaxx,Mminy:Mmaxy,Gnk), &
           F_w(Mminx:Mmaxx,Mminy:Mmaxy,Gnk), F_s (Mminx:Mmaxx,Mminy:Mmaxy    )
!
!author 
!     Michel Roch - rpn - oct 1993 
!
!revision
! v2_00 - Desgagne M.       - initial MPI version (from predat1 v1_03)
! v2_30 - Edouard  S.       - adapt for vertical hybrid coordinate
! v2_31 - Desgagne M.       - remove treatment of HUT1 and QCT1
! v3_00 - Desgagne & Lee    - Lam configuration
! v3_02 - Desgagne M.       - correction for non-hydrostatic version
! v3_03 - Tanguay M.        - put v4d_predat inside 
! v3_21 - Tanguay M.        - do filtering at t=0 in v4d_predat
! v3_21 - Desgagne M.       - Revision OpenMP
! v3_22 - Belanger/Lee      - Introduce vtap
! v3_22 - Tanguay M.        - Change postitioning of hzd_hoffld for zd
! v3_30 - Lee V.            - remove call to rpn_comm_xch
! v4_00 - Plante A.         - Modif for staggering formulation 1 C. Girard.
!                             Note : s is now computed in readdyn.ftn or 
!                             mtn_case.ftn or bubble.ftn 
! v5_00 - Lee V.            - Removed VTAP
! v4_04 - Tanguay M.        - Staggered version TL/AD 
! v4_05 - Plante A.         - Remove zeroing of w,zd and q (initialized 
!                             in the entrance)
! v4_41 - Plante A.         - call diag_zd_w
!
!
#include "gmm.hf"
#include "glb_ld.cdk"
#include "bcsgrds.cdk"
#include "grd.cdk"
#include "nest.cdk"
      
!
      type(gmm_metadata) :: mymeta
      integer i,j,k,istat
!     __________________________________________________________________
!
      call diag_zd_w ( F_zd,F_w(l_minx,l_miny,2), F_u,F_v  , &
                       F_t(l_minx,l_miny,2), F_s, LDIST_DIM, &
                            l_nk,.not.Ana_zd_L,.not.Ana_w_L)
      if (Grd_yinyang_L) then
          if (.not. Ana_zd_L) then
              istat = gmm_get(gmmk_nest_zd_s ,nest_zd ,mymeta)
              do k= 1, G_nk+1
              do j= 1, l_nj
              do i= 1, l_ni
                 nest_zd(i,j,k) =F_zd(i,j,k)
              enddo
              enddo
              enddo
              call rpn_comm_xch_halo(nest_zd, LDIST_DIM,l_ni,l_nj,G_nk+1, &
                  G_halox,G_haloy,G_periodx,G_periody,l_ni,0 )
              call yyg_scalbc(F_zd,nest_zd,LDIST_DIM,G_nk+1)
          endif
          if (.not. Ana_w_L) then
              istat = gmm_get(gmmk_nest_w_s ,nest_w ,mymeta)
              do k= 1, G_nk+1
              do j= 1, l_nj
              do i= 1, l_ni
                 nest_w(i,j,k) =F_w(i,j,k)
              enddo
              enddo
              enddo
              call rpn_comm_xch_halo(nest_w, LDIST_DIM,l_ni,l_nj,G_nk+1, &
                  G_halox,G_haloy,G_periodx,G_periody,l_ni,0 )
              call yyg_scalbc(F_w,nest_w,LDIST_DIM,G_nk+1)
          endif
      endif
!
!     __________________________________________________________________
!
      return
      end
