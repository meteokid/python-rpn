!**s/r e_specanal2 - extraction of analysis levels
!
#include "model_macros_f.h"
!
      subroutine e_specanal2 (current_date, errcode)
      implicit none
#include <arch_specific.hf>
!
      integer current_date, errcode
!author 
!    Michel Desgagne - Fall 2012
!
!revision
! v4_50 - Desgagne M.         - initial version
!
#include "e_anal.cdk"
#include "e_fu.cdk"
#include "e_grids.cdk"
#include "grd.cdk"
#include "lun.cdk"
#include "filename.cdk"
#include "pilot.cdk"

      integer,  external :: px_param, e_fstouv
      integer, parameter :: PRESVAR_MAX = 6, nlist = 2000
      character*4 var_uu,names(10)
      character*6 presvar_S(PRESVAR_MAX)
      character*8 vcode_S
      integer err, err_uu, err_vt, err_hu, vcode, list(nlist), istat, k, nka, ni1
      real*8  presvar_8(PRESVAR_MAX)
      real*8, dimension(:), allocatable:: hyb_8,hyb_t_8
!Notes for coding anal_hav (from px_param)
!                        0=pressure
!                        1=SIGMA
!                        2=ETASEF (eta for spectral)
!                        3=SIGPT  (eta, rcoef=1.0)
!                        4=HYBLG  (hybrid Laprise/Girard)
!                        5=ECMWF  (data from ECMWF)
!                        6=HYBSTAG(staggered hybrid Girard)
!
! ---------------------------------------------------------------------
!
      errcode = -1

      write (6, 1001)

      err= e_fstouv ('P0', current_date, Grd_typ_S(1:1), &
                                  pilot_dir, e_fu_anal)
      if (err.lt.0) &
      err= e_fstouv ('TT', current_date, Grd_typ_S(1:1), &
                                  pilot_dir, e_fu_anal)

      if (err.lt.0) then
         if (Lun_out.gt.0) write (6,*) 'E_SPECANAL: Problem with e_fstouv'
         return
      endif

      names(1)= 'UU' ; names(2)= 'UT1'
      call e_whichvar (names,2,e_fu_anal,current_date,var_uu,err_uu)
      names(1)= 'VT' ; names(2)= 'TT'
      call e_whichvar (names,2,e_fu_anal,current_date,vt,err_vt)
      names(1)= 'HU' ; names(2)= 'ES'; names(3)= 'TD' ; names(4)= 'HR'
      call e_whichvar (names,4,e_fu_anal,current_date,vh,err_hu)

      if ( (err_uu.lt.0).or.(err_vt.lt.0).or.(err_hu.lt.0) ) then
         if (Lun_out.gt.0) write (6,*) 'E_SPECANAL: Problem with e_whichvar'
         return
      endif

      nia = -1 ; nka = -1
      call e_hvgrid (vt,     current_date, e_fu_anal, nia,nja,nka, &
                      grda, ig1a,ig2a,ig3a,ig4a,list,nlist,err_vt)

      if (err_vt.ge.0) then
         allocate ( hyb_t_8(nka) ) ; hyb_t_8=-1.
         err = px_param( e_fu_anal,list,na_t,nka,lv_t,hyb_t_8, 'T',&
                         presvar_8,presvar_S,PRESVAR_MAX,          &
                         zat_8,zbt_8,vcode_S,vcode )
      endif

      ni1 = nia ; nka = -1
      if (trim(var_uu)=='UT1') ni1=0
      call e_hvgrid (var_uu, current_date, e_fu_anal, ni1,nja,nka, &
                      grda, ig1a,ig2a,ig3a,ig4a,list,nlist,err_uu)

      if (err_uu.ge.0) then
         allocate ( hyb_8(nka) ) ; hyb_8 = -1.
         err = px_param( e_fu_anal,list,na,nka,lv,hyb_8,  'M',&
                         presvar_8,presvar_S,PRESVAR_MAX,     &
                         za_8,zb_8,vcode_S,vcode )
      endif

      if ( (err_uu.lt.0).or.(err_vt.lt.0) ) then
         if (Lun_out.gt.0) write (6,*) 'E_SPECANAL: Problem with e_hvgrid'
         return
      endif      

      Anal_sigma_L   = .false.
      Anal_eta_L     = .false. 
      Anal_hyb_L     = .false.
      Anal_pres_L    = .false.
      Anal_hybstg_L  = .false.
      anal_hav(2)    = vcode

      if (vcode_S.eq."HYBLG")   Anal_hyb_L    = .true. !4
      if ((vcode_S.eq."SIGPT").or.(vcode_S.eq."ETASEF"))   Anal_eta_L    = .true. !2-3
      if (vcode_S.eq."SIGMA")   Anal_sigma_L  = .true. !1
      if (vcode_S.eq."PRESS")   Anal_pres_L   = .true. !0
      if (vcode_S.eq."HYBSTAG") Anal_hybstg_L = .true. !6

      prefa    = presvar_8(1)
      ptopa    = presvar_8(2)
      rcoefa(1)= presvar_8(4)
      rcoefa(2)= presvar_8(5)

      print *,'===> Analysis on ',vcode_S,' vertical coordinates'

      if ( (Anal_sigma_L.or.Anal_eta_L.or.Anal_hyb_L.or.Anal_hybstg_L) &
            .and. ( VH .ne. 'HU' ) ) then
         write(6,*) 'E_SPECANAL: Moisture variable(VH) must be HU '
         write(6,*) 'E_SPECANAL: when analysis is on sigma/eta/hybrid.'
         return
      endif

      if (LV.eq.1) then
         print*," A SHALLOW WATER model is now to be ASSUMED "
         print*," VERIFY if SET Schm_autobar_L=.true.        "
         LV_T=1
         rna_t(1)=rna(1)
         na_t(1)=na(1)
      endif

      print*, var_uu,lv,'ANALYSIS LEVELS FOUND ARE:'
      do k=1,lv
         rna(k)=hyb_8(k)
         write(6,801) rna(k),k,na(k)
      end do 

      print*, vt,lv_t,'ANALYSIS LEVELS FOUND ARE:'
      do k=1,lv_t
         rna_t(k)=hyb_t_8(k)
         write(6,801) rna_t(k),k,na_t(k)
      end do 

      deallocate (hyb_8,hyb_t_8)

!     Check for vertical consistency when (.not.Anal_hybstg_L)
      istat = 0
      if (.not.Anal_hybstg_L) then
         if (lv.ne.lv_t) then
            print *,'Levels found for TT/VT is',LV_t,': should be ',LV
            istat = -1
         endif
         do k=1,lv
            if (rna(k).ne.rna_t(k)) then
               print *,'Level',k,'for UU is',rna(k),' which is not the same for TT/VT ',rna_t(k)
               istat = -1
            endif
         enddo
      endif

      if (istat.lt.0) return

!     Check presence of ZDT1, WT1 and QT1 and initialyse nvar

      nvar = 5
      VAR_ZD='' ; VAR_W='' ; VAR_Q=''

      if ( .not. Anal_pres_L ) then
         nka = lv_t ; list(1) = -1
         call e_hvgrid ('ZDT1',current_date, e_fu_anal, nia,nja,nka, &
              grda, ig1a,ig2a,ig3a,ig4a,list,nlist,err)
         if (list(1).ge.0) then
            nvar=nvar+1
            VAR_ZD='ZDT1'
         endif
         
         list(1) = -1
         call e_hvgrid ('WT1',current_date, e_fu_anal, nia,nja,nka, &
              grda, ig1a,ig2a,ig3a,ig4a,list,nlist,err)
         if (list(1).ge.0) then
            nvar=nvar+1
            VAR_W='WT1'
         else
            call e_hvgrid ('ZZ' ,current_date, e_fu_anal, nia,nja,nka, &
                 grda, ig1a,ig2a,ig3a,ig4a,list,nlist,err)
            if (list(1).ge.0) then
               nvar=nvar+1
               VAR_W='ZZ'
            endif
         endif
         
         nka = lv ; list(1) = -1
         call e_hvgrid ('QT1',current_date, e_fu_anal, nia,nja,nka, &
              grda, ig1a,ig2a,ig3a,ig4a,list,nlist,err)
         if (list(1).ge.0) then
            nvar=nvar+1
            VAR_Q='QT1'
         endif
      endif

      istat = 0
      if (Anal_pres_L.and.lv.gt.1.and.lv.lt.16) then
         write(6,*) ': NEED 16 OR MORE PRESSURE LEVELS. ', &
                    lv, ' levels found'
         istat = -1
      else if (Anal_sigma_L.or.Anal_eta_L.or.Anal_hyb_L.or.Anal_hybstg_L) then
         if (rna(lv).ne.1.0) then
            write(6,*)'**************************************'
            write(6,*)'rna(',lv,') is not equal to 1.0'
            write(6,*)'WARNING: NO topography level found!!!!'
            write(6,*)'**************************************'
         endif
      endif

      if (istat .lt.0) return

      call e_target3df (grda, nia, nja, ig1a, ig2a, ig3a, ig4a)

      errcode = 0

 801  format (' LEVEL anal = ',d15.8,2X,' k = ',i4,' IP1= ',I8)
 1001 format(/,'OBTAIN INPUT DATA LEVEL STRUCTURE (S/R E_SPECANAL)',/50('-'))
!
! ---------------------------------------------------------------------
!
      return
      end
