!
!NOTE: the equivalence statement below MUST mark the
!FIRST element of the common block or the OUTPUTS will be wrong.
!If you introduce an element in the beginning of this block, make
!changes to assure the equivalence statement below MARKS it.
!
      real, pointer, dimension (:,:,:) :: rhsu_m     ! effective dimensions are (LDIST_SHAPE,  l_nk  )
      real, pointer, dimension (:,:,:) :: rhsv_m     ! effective dimensions are (LDIST_SHAPE,  l_nk  )
      real, pointer, dimension (:,:,:) :: rhst_m     ! effective dimensions are (LDIST_SHAPE,  l_nk+1)
      real, pointer, dimension (:,:,:) :: rhsc_m     ! effective dimensions are (LDIST_SHAPE,  l_nk  )
      real, pointer, dimension (:,:,:) :: rhsw_m     ! effective dimensions are (LDIST_SHAPE,  l_nk+1)
      real, pointer, dimension (:,:,:) :: rhsf_m     ! effective dimensions are (LDIST_SHAPE,  l_nk+1)
      real, pointer, dimension (:,:,:) :: rhsp_m     ! effective dimensions are (LDIST_SHAPE,  l_nk  )
      real, pointer, dimension (:,:  ) :: rhsb_m     ! effective dimensions are (LDIST_SHAPE         )
      real, pointer, dimension (:,:,:) :: ruw1_m     ! effective dimensions are (LDIST_SHAPE,  l_nk  )
      real, pointer, dimension (:,:,:) :: rvw1_m     ! effective dimensions are (LDIST_SHAPE,  l_nk  )
      real, pointer, dimension (:,:,:) :: ruw2_m     ! effective dimensions are (LDIST_SHAPE,  l_nk  )
      real, pointer, dimension (:,:,:) :: rvw2_m     ! effective dimensions are (LDIST_SHAPE,  l_nk  )
!
      common /rhsc_m_gmm/ rhsu_m,rhsv_m,rhst_m,rhsc_m,rhsw_m,rhsf_m,rhsp_m,rhsb_m
      common /rhsc_m_gmm/ ruw1_m,rvw1_m,ruw2_m,rvw2_m
