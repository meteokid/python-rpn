      character(len=8) :: gmmk_rhsu_m_s,gmmk_rhsv_m_s,gmmk_rhst_m_s,gmmk_rhsc_m_s
      character(len=8) :: gmmk_rhsw_m_s,gmmk_rhsf_m_s,gmmk_rhsp_m_s,gmmk_rhsb_m_s
      character(len=8) :: gmmk_ruw1_m_s,gmmk_rvw1_m_s,gmmk_ruw2_m_s,gmmk_rvw2_m_s
!
      common /rhsc_m_n_gmm/ gmmk_rhsu_m_s,gmmk_rhsv_m_s,gmmk_rhst_m_s,gmmk_rhsc_m_s
      common /rhsc_m_n_gmm/ gmmk_rhsw_m_s,gmmk_rhsf_m_s,gmmk_rhsp_m_s,gmmk_rhsb_m_s
      common /rhsc_m_n_gmm/ gmmk_ruw1_m_s,gmmk_rvw1_m_s,gmmk_ruw2_m_s,gmmk_rvw2_m_s
