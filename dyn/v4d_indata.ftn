      subroutine v4d_indata1 (F_vtopo_L)
      implicit none
*
      logical F_vtopo_L
*
#include "v4dg.cdk"
*
*-------------------------------------------------------------------
*
      if ( V4dg_conf.ne.0) then
           if ( F_vtopo_L) 
     %     call gem_stop ('v4d_INDATA: V4dg_conf.ne.0.and.Vtopo_L NOT VALID', -1)
           if (V4dg_part.eq.3.and.V4dg_oktr_L ) call v4d_rwtraj (1) 
*
      endif
*
