*
      return
      end
*
      subroutine v4d_indata2(skipcode)
      implicit none
      integer skipcode
*
*
#include "v4dg.cdk"
*
*-------------------------------------------------------------------
*
      if ( V4dg_conf.ne.0 .and. V4dg_part.ne.2 ) then 
           call predat ()
           skipcode=1
      endif
*
*-------------------------------------------------------------------
*
      return
      end
