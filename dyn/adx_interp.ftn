***s/r adx_interp - Interpolation of one rhs
*
#include "model_macros_f.h"
*
      subroutine adx_interp ( F_out, F_in, F_u, F_v, 
     %                        F_wind_L, F_mono_L, DIST_DIM, F_nk,
     %                        i0,in,j0,jn, F_lev_S )
*
      implicit none
*
      character*1 F_lev_S
*
      logical F_wind_L, F_mono_L
*
      integer DIST_DIM, F_nk, i0,in,j0,jn
*
      real F_out (DIST_SHAPE, F_nk),
     %     F_in  (DIST_SHAPE, F_nk)
      real F_u(*), F_v(*)
*
*author
*     alain patoine
*
*revision
* v2_31 - Tanguay M.        - correction parameters adx_vder 
* v3_00 - Desgagne & Lee    - Lam configuration
* v3_10 - Corbeil & Desgagne & Lee - AIXport+Opti+OpenMP
* v3_20 - Gravel & Valin & Tanguay - Lagrange 3D
* v3_20 - McTaggart-Cowan   - Add semi-cubic Lagrangian interpolation
* v3_30 - Desgagne          - Moved semi-cubic Lag interp to adx_interp2
*
*language
*     fortran 77
*
*object
*     see id section
*
*arguments
*______________________________________________________________________
*              |                                                 |     |
* NAME         | DESCRIPTION                                     | I/O |
*--------------|-------------------------------------------------|-----|
*              |                                                 |     |
* F_out        | interpolated field                              |  o  |
* F_in         | field to interpolate                            |  i  |
* F_wind_L     | switch: .true. : field to interpolate is a wind |  i  |
*              |                  like quantity                  |  i  |
* F_mono_L     | switch: .true. : monotonic interpolation        |  i  |
*______________|_________________________________________________|_____|
*
*implicits
#include "glb_ld.cdk"
#include "type.cdk"
#include "adx.cdk"
*
************************************************************************
      integer i, j, k, nij, nijk, nijkag, n, dest_ni
*
      real dummy
*
      nij    = l_ni   *l_nj
      nijk   = l_ni   *l_nj   *F_nk
      nijkag = adx_nit*adx_njt*F_nk
*
*
************************************************************************
*
* Adjust field to advection grid
*
* Compute extension beyond the pole if appropriate
*
************************************************************************
      if (G_lam) then
          n=0
          dest_ni=l_ni
      else
          n=999
          dest_ni=G_ni
      endif
      call rpn_comm_xch_halox (F_in, LDIST_DIM, l_ni, l_nj, F_nk,
     % adx_halox, adx_haloy, G_periodx, G_periody, F_u, -adx_halox+1,
     % adx_nic+adx_halox, -adx_haloy+1, adx_njc+adx_haloy, dest_ni, n)
*
      if (.not.G_lam) then
      if ( l_south ) then
*     
         if ( F_wind_L ) then
            call adx_pol0 (F_u, 0, adx_nic,adx_halox,adx_njc,
     %                     adx_haloy,F_nk)
         else
            call adx_pols (F_u,adx_wx_8, 0, adx_nic,adx_halox,
     %                     adx_njc,adx_haloy,F_nk)
         endif
            call adx_polx (F_u,adx_xg_8,.true.,adx_nic,adx_halox,
     %                     adx_njc,adx_haloy,F_nk)
      endif
*     
      if ( l_north ) then
*
         if ( F_wind_L ) then
            call adx_pol0 (F_u,adx_njc+1,adx_nic,adx_halox,adx_njc,
     %                     adx_haloy,F_nk)
         else
            call adx_pols (F_u,adx_wx_8, adx_njc+1,adx_nic,adx_halox,
     %                     adx_njc,adx_haloy,F_nk)
         endif
            call adx_polx (F_u,adx_xg_8,.false.,adx_nic,adx_halox,
     %                     adx_njc,adx_haloy,F_nk)
      endif
      endif
************************************************************************
*
* Interpolate
*
************************************************************************
      call adx_vder ( F_v, F_u, adx_nit, adx_njt, F_nk, F_lev_S ) !2nd deriv for spline
      call adx_tricub ( adx_wrkc, F_u, F_v,
     %                  adx_n1, adx_capx1, adx_xgg1,
     %                  adx_xdd1, adx_capy1, adx_ygg1,
     %                  adx_ydd1, adx_capz1,adx_cz1,
     %                  nijk, F_mono_L,i0,in,j0,jn,F_nk)
*
      if (.not.G_lam) then
      if ( adx_fro_a .gt. 0 ) then
         call adx_tricub ( adx_wrka, F_u, F_v,
     %                     adx_n2, adx_capx2, adx_xgg2,
     %                     adx_xdd2, adx_capy2, adx_ygg2,
     %                     adx_ydd2, adx_capz2, adx_cz2,
     %                     adx_fro_a, F_mono_L,1,adx_fro_a,1,1,1)
      endif
*
      call adx_exch_2 ( adx_wrkb, dummy, dummy,
     %                  adx_wrka, dummy, dummy,
     %                  adx_for_n, adx_for_s, adx_for_a,
     %                  adx_fro_n, adx_fro_s, adx_fro_a, 1)
*
      if ( adx_for_a .gt. 0 )
     %   call adx_exch_3 ( adx_wrkc, dummy, adx_wrkb, dummy, adx_c1, 1 )
*
      endif
!$omp parallel
!$omp do
      do k = 1, F_nk
      do j = j0,jn
      do i = i0,in
      F_out(i,j,k) = adx_wrkc ( (k-1)*nij+(j-1)*l_ni+i )
      enddo
      enddo
      enddo
!$omp enddo
!$omp end parallel
*
      return
      end
