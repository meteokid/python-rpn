***s/r nest_bcs -
*
#include "model_macros_f.h"
*
      subroutine nest_bcs ()
      implicit none
*
*author 
*     Michel Desgagne   - Spring 2006
*
*revision
* v3_30 - Lee V.          - initial version
* v4_03 - Lee/Plante      - acid test
* v4_05 - Lepine M.       - VMM replacement with GMM
*
*object
*
*arguments
*       none
*
*implicits
#include "gmm.hf"
#include "var_gmm.cdk"
#include "glb_ld.cdk"
#include "bcsdim.cdk"
#include "bcsgrds.cdk"
#include "bcsmem.cdk"
#include "vt1.cdk"
#include "nest.cdk"
#include "tr3d.cdk"
#include "lam.cdk"
#include "schm.cdk"
#include "acid.cdk"
*
      character*16 tr_name
      integer err,i,j,k,nvar,n
      real, pointer, dimension(:,:,:) :: tr,tr0

*----------------------------------------------------------------------
*
      if (north+south+west+east.lt.1) then
*
         if (Lam_toptt_L) then
*
*           Pilot the temperature for the whole top level
            gmmstat = gmm_get(gmmk_nest_t_s,nest_t,meta3d)
            if (GMM_IS_ERROR(gmmstat)) print *,'nest_bcs ERROR at gmm_get(nest_t)'
            gmmstat = gmm_get(gmmk_tt1_s,tt1,meta3d)
            if (GMM_IS_ERROR(gmmstat)) print *,'nest_bcs ERROR at gmm_get(tt1)'
*
            do j=1,l_nj
            do i=1,l_ni
               tt1(i,j,1) = nest_t(i,j,1)
            end do
            end do
*
         endif
*
         return

      endif
*
      gmmstat = gmm_get(gmmk_nest_u_s ,nest_u ,meta3d)
      if (GMM_IS_ERROR(gmmstat)) print *,'nest_bcs ERROR at gmm_get(nest_u)'
      gmmstat = gmm_get(gmmk_nest_v_s ,nest_v ,meta3d)
      if (GMM_IS_ERROR(gmmstat)) print *,'nest_bcs ERROR at gmm_get(nest_v)'
      gmmstat = gmm_get(gmmk_nest_w_s ,nest_w ,meta3d)
      if (GMM_IS_ERROR(gmmstat)) print *,'nest_bcs ERROR at gmm_get(nest_w)'
      gmmstat = gmm_get(gmmk_nest_t_s ,nest_t ,meta3d)
      if (GMM_IS_ERROR(gmmstat)) print *,'nest_bcs ERROR at gmm_get(nest_t)'
      gmmstat = gmm_get(gmmk_nest_zd_s,nest_zd,meta3d)
      if (GMM_IS_ERROR(gmmstat)) print *,'nest_bcs ERROR at gmm_get(nest_zd)'
      gmmstat = gmm_get(gmmk_nest_s_s ,nest_s ,meta2d)
      if (GMM_IS_ERROR(gmmstat)) print *,'nest_bcs ERROR at gmm_get(nest_s)'

      gmmstat = gmm_get(gmmk_ut1_s,ut1,meta3d)
      if (GMM_IS_ERROR(gmmstat)) print *,'nest_bcs ERROR at gmm_get(ut1)'
      gmmstat = gmm_get(gmmk_vt1_s,vt1,meta3d)
      if (GMM_IS_ERROR(gmmstat)) print *,'nest_bcs ERROR at gmm_get(vt1)'
      gmmstat = gmm_get(gmmk_wt1_s,wt1,meta3d)
      if (GMM_IS_ERROR(gmmstat)) print *,'nest_bcs ERROR at gmm_get(wt1)'
      gmmstat = gmm_get(gmmk_tt1_s,tt1,meta3d)
      if (GMM_IS_ERROR(gmmstat)) print *,'nest_bcs ERROR at gmm_get(tt1)'
      gmmstat = gmm_get(gmmk_zdt1_s,zdt1,meta3d)
      if (GMM_IS_ERROR(gmmstat)) print *,'nest_bcs ERROR at gmm_get(zdt1)'
      gmmstat = gmm_get(gmmk_st1_s,st1,meta2d)
      if (GMM_IS_ERROR(gmmstat)) print *,'nest_bcs ERROR at gmm_get(st1)'
      if (.not.Schm_hydro_L) then
         gmmstat = gmm_get(gmmk_nest_q_s,nest_q,meta3d)
         if (GMM_IS_ERROR(gmmstat)) print *,'nest_bcs ERROR at gmm_get(nest_q)'
         gmmstat = gmm_get(gmmk_qt1_s,qt1,meta3d)
         if (GMM_IS_ERROR(gmmstat)) print *,'nest_bcs ERROR at gmm_get(qt1)'
      endif
*
      if (l_north) then
         do k=1,G_nk+1
            do j=l_nj-pil_n+1,l_nj
            do i=1,l_ni
               wt1  (i,j,k) = nest_w  (i,j,k)
               tt1  (i,j,k) = nest_t  (i,j,k)
               zdt1 (i,j,k) = nest_zd (i,j,k)
            end do
            end do
            do j=l_nj-pil_n+1,l_nj
            do i=1,l_niu
               ut1  (i,j,k) = nest_u (i,j,k)
            end do
            end do
            do j=l_nj-pil_n,l_njv
            do i=1,l_ni
               vt1  (i,j,k) = nest_v (i,j,k)
            end do
            end do
         end do
         do j=l_nj-pil_n+1,l_nj
         do i=1,l_ni
            st1  (i,j) = nest_s (i,j)
            tt1  (i,j,G_nk+2) = nest_t (i,j,G_nk+2)
         end do
         end do
         if (.not. Schm_hydro_L) then
            do k=0,G_nk+1
            do j=l_nj-pil_n+1,l_nj
            do i=1,l_ni
               qt1(i,j,k)  = nest_q(i,j,k)
            end do
            end do
            end do
         endif
      endif
*
      if (l_east) then
         do k=1,G_nk+1
            do j=1,l_nj
            do i=l_ni-pil_e+1,l_ni
               wt1  (i,j,k) = nest_w  (i,j,k)
               tt1  (i,j,k) = nest_t  (i,j,k)
               zdt1 (i,j,k) = nest_zd (i,j,k)
            end do
            end do
            do j=1,l_nj
            do i=l_ni-pil_e,l_niu
               ut1  (i,j,k) = nest_u  (i,j,k)
            end do
            end do
            do j=1,l_njv
            do i=l_ni-pil_e+1,l_ni
               vt1  (i,j,k) = nest_v  (i,j,k)
            end do
            end do
         end do
         do j=1,l_nj
         do i=l_ni-pil_e+1,l_ni
            st1  (i,j) = nest_s  (i,j)
            tt1  (i,j,G_nk+2) = nest_t  (i,j,G_nk+2)
         end do
         end do
         if (.not. Schm_hydro_L) then
            do k=0,G_nk+1
            do j=1,l_nj
            do i=l_ni-pil_e+1,l_ni
               qt1(i,j,k)  = nest_q(i,j,k)
            end do
            end do
            end do
         endif
      endif
*
      if (l_south) then
         do k=1,G_nk+1
            do j=1,pil_s
            do i=1,l_ni
               wt1  (i,j,k) = nest_w  (i,j,k)
               vt1  (i,j,k)= nest_v  (i,j,k)
               tt1  (i,j,k) = nest_t  (i,j,k)
               zdt1 (i,j,k) = nest_zd (i,j,k)
            end do
            end do
            do j=1,pil_s
            do i=1,l_niu
               ut1  (i,j,k) = nest_u  (i,j,k)
            end do
            end do
         end do
         do j=1,pil_s
         do i=1,l_ni
            st1  (i,j) = nest_s  (i,j)
            tt1  (i,j,G_nk+2) = nest_t  (i,j,G_nk+2)
         end do
         end do
         if (.not. Schm_hydro_L) then
            do k=0,G_nk+1
            do j=1,pil_s
            do i=1,l_ni
               qt1(i,j,k)  = nest_q(i,j,k)
            end do
            end do
            end do
         endif
      endif
*
      if (l_west) then
         do k=1,G_nk+1
            do j=1,l_nj
            do i=1,pil_w
               wt1  (i,j,k) = nest_w  (i,j,k)
               ut1  (i,j,k) = nest_u  (i,j,k)
               tt1  (i,j,k) = nest_t  (i,j,k)
               zdt1 (i,j,k) = nest_zd (i,j,k)
            end do
            end do
            do j=1,l_njv
            do i=1,pil_w
               vt1  (i,j,k) = nest_v  (i,j,k)
            end do
            end do
         end do
         do j=1,l_nj
         do i=1,pil_w
            st1  (i,j) = nest_s  (i,j)
            tt1  (i,j,G_nk+2) = nest_t  (i,j,G_nk+2)
         end do
         end do
         if (.not. Schm_hydro_L) then
            do k=0,G_nk+1
            do j=1,l_nj
            do i=1,pil_w
               qt1(i,j,k) = nest_q(i,j,k)
            end do
            end do
            end do
         endif
      endif
*
      if (Lam_toptt_L) then
*        Pilot the temperature for the whole top level
         do j=1,l_nj
         do i=1,l_ni
            tt1(i,j,1) = nest_t(i,j,1)
         end do
         end do
      endif
*
      do n=1,Tr3d_ntr
         tr_name = 'NEST/'//trim(Tr3d_name_S(n))//':C'
      	 gmmstat = gmm_get(tr_name,tr,meta3d)
         tr_name = 'TR/'//trim(Tr3d_name_S(n))//':P'
      	 gmmstat = gmm_get(tr_name,tr0,meta3d)

         if (l_north) then
            do k=1,G_nk+2
            do j=l_nj-pil_n+1,l_nj
            do i=1,l_ni
               tr0  (i,j,k) = tr  (i,j,k)
            enddo
            enddo
            enddo
         endif
         if (l_east) then
            do k=1,G_nk+2
            do j=1,l_nj
            do i=l_ni-pil_e+1,l_ni
               tr0  (i,j,k) = tr  (i,j,k)
            enddo
            enddo
            enddo
         endif
         if (l_south) then
            do k=1,G_nk+2
            do j=1,pil_s
            do i=1,l_ni
               tr0  (i,j,k) = tr  (i,j,k)
            enddo
            enddo
            enddo
         endif
         if (l_west) then
            do k=1,G_nk+2
            do j=1,l_nj
            do i=1,pil_w
               tr0  (i,j,k) = tr  (i,j,k)
            enddo
            enddo
            enddo
         endif
c
c            if(acid_test_L)call acid_stat_nest_bcs(tr0,tr,n,1)
c
      enddo
*
*----------------------------------------------------------------------
      return
 1000 format (/,19('#'),' NEST_BCS STAT ',i6,1X,19('#'))
 1001 format(3X,'NEST Boundary ConditionS: (S/R NEST_BCS)')
      end
