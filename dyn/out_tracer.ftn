***s/r out_tracer - calculate and output tracer fields
*
#include "model_macros_f.h"
*
      subroutine out_tracer (F_wlnph_ta,DIST_DIM,F_nk,F_levtyp_S,
     %                       F_rf,F_indo,F_nkot,F_set)
*
      implicit none
*
      character*1 F_levtyp_S
      integer F_nk,DIST_DIM,F_nkot,F_indo(*),F_set
      real F_wlnph_ta(DIST_SHAPE,F_nk+2), F_rf(*)
*
*author
*     Lee V.                    - rpn May 2004
*
*revision
* v3_20 - Lee V.            - initial MPI version (from bloctr     v3_12)
* v3_30 - Lee V.            - option to clip tracers with Out3_cliph_L
* v4_05 - Lee V.            - adaptation to GMM
*
*object
*     output all the tracer fields
*	
*arguments
*  Name        I/O                 Description
*----------------------------------------------------------------
* F_dostep     I    - array containing indices corresponding to the
*                     timestep sets that requires output at this time step.
* F_dostep_max I    - size of F_dostep array
*
*
*implicits
#include "gmm.hf"
#include "var_gmm.cdk"
#include "glb_ld.cdk"
#include "lun.cdk"
#include "out3.cdk"
#include "out.cdk"
#include "outd.cdk"
#include "tr3d.cdk"
#include "type.cdk"
#include "ver.cdk"
*
*
**
      integer i,j,k, ii,n
      integer i0,in,j0,jn
*
*
      real prprlvl(F_nkot)
      real w4(minx:maxx,miny:maxy,F_nkot) 
      real t4(minx:maxx,miny:maxy,F_nk+2) 
      logical outvar_L
      real, pointer    , dimension(:,:,:) :: tr1
*
*_______________________________________________________________________
*
      i0 = 1
      in = l_ni
      j0 = 1
      jn = l_nj
*
      if (F_levtyp_S .eq. 'M') then
*_______________________________________________________________________
*
*     1.0A   Output of tracer variables on ETA levels
*_______________________________________________________________________
*
      do ii=1,Outd_var_max(F_set)
         outvar_L=.false.
         do n=1,Tr3d_ntr
           if (Outd_var_S(ii,F_set)(1:2).eq.trim(Tr3d_name_S(n))) then
               if (Outd_var_S(ii,F_set)(3:4).eq.'T0') then
                   nullify (tr1)
                   gmmstat=gmm_get('TR/'//trim(Tr3d_name_S(n))//':M',tr1,meta3d)
                   if (.not.GMM_IS_ERROR(gmmstat)) outvar_L=.true.
                elseif (Outd_var_S(ii,F_set)(3:4).eq.'T1') then
                   nullify (tr1)
                   gmmstat=gmm_get('TR/'//trim(Tr3d_name_S(n))//':P',tr1,meta3d)
                   if (.not.GMM_IS_ERROR(gmmstat)) outvar_L=.true.
                endif
                cycle
           endif
         enddo
         if (outvar_L) then
             if (Out3_cliph_L) then
                 do k=1,F_nk+2
                 do j=1,l_nj
                 do i=1,l_ni
                    t4(i,j,k) = amax1(tr1(i,j,k), 0. )
                 enddo
                 enddo
                 enddo
                 call ecris_fst2(t4,l_minx,l_maxx,l_miny,l_maxy,Ver_hyb%t,
     $             Outd_var_S(ii,F_set),1.0,0.0,Out_kind,F_nk+2,F_indo,F_nkot, 
     $             Outd_nbit(ii,F_set) )
             else
                 call ecris_fst2(tr1,l_minx,l_maxx,l_miny,l_maxy,Ver_hyb%t,
     $             Outd_var_S(ii,F_set),1.0,0.0,Out_kind,F_nk+2,F_indo,F_nkot, 
     $             Outd_nbit(ii,F_set) )
             endif
         endif
      enddo

      else
*_______________________________________________________________________
*
*     1.0B   Output of tracer variables on PRESSURE levels
*_______________________________________________________________________
*
      do i = 1, F_nkot
         prprlvl(i) = F_rf(i) * 100.0
      enddo
      do ii=1,Outd_var_max(F_set)
         outvar_L=.false.
         do n=1,Tr3d_ntr
           if (Outd_var_S(ii,F_set)(1:2).eq.trim(Tr3d_name_S(n))) then
               if (Outd_var_S(ii,F_set)(3:4).eq.'T0') then
                   nullify (tr1)
                   gmmstat=gmm_get('TR/'//trim(Tr3d_name_S(n))//':M',tr1,meta3d)
                   if (.not.GMM_IS_ERROR(gmmstat)) outvar_L=.true.
                elseif (Outd_var_S(ii,F_set)(3:4).eq.'T1') then
                   nullify (tr1)
                   gmmstat=gmm_get('TR/'//trim(Tr3d_name_S(n))//':P',tr1,meta3d)
                   if (.not.GMM_IS_ERROR(gmmstat)) outvar_L=.true.
                endif
                cycle
           endif
         enddo
         if (outvar_L) then
             call verder (t4,tr1,F_wlnph_ta,2.0,2.0,
     $                   l_minx,l_maxx,l_miny,l_maxy,F_nk+2,i0,in,j0,jn)
             call prgen( w4, tr1, t4, F_wlnph_ta, prprlvl,F_nkot,
     %                   Out3_cubzt_L, l_minx,l_maxx,l_miny,l_maxy, F_nk+2)
            if (Outd_filtpass(ii,F_set).gt.0)
     $          call filter(w4,Outd_filtpass(ii,F_set),
     $                      Outd_filtcoef(ii,F_set),'G', .false.,
     $                      l_minx,l_maxx,l_miny,l_maxy, F_nkot)
            if (Out3_cliph_L) then
               do k=1,F_nkot
               do j=1,l_nj
               do i=1,l_ni
                  w4(i,j,k) = amax1(w4(i,j,k), 0. )
               enddo
               enddo
               enddo
            endif
            call ecris_fst2(w4,l_minx,l_maxx,l_miny,l_maxy,F_rf,
     $         Outd_var_S(ii,F_set),1.0,0.0,Out_kind,F_nkot, 
     $         F_indo, F_nkot, Outd_nbit(ii,F_set) )

        endif
      enddo

      endif
* ___________________________________________________________________
*

      return
      end
