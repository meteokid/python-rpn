***s/r v4d_predat_tl - TLM of v4d_predat 
*
#include "model_macros_f.h"
*
      subroutine v4d_predat_tl
*
      implicit none
*
*author
*     M.Tanguay
*
*revision
* v3_03 - Tanguay M.        - initial MPI version
* v3_11 - Tanguay M.        - modify evaluation of Ind_pip
* v3_21 - Tanguay M.        - Revision Openmp 
*                           - do filtering at t=0 in v4d_predat
* v3_22 - Tanguay M.        - Change positioning of hzd_hoffld for psd 
* v3_30 - Tanguay M.        - Change parameters of initw2 
* v4_04 - Tanguay M.        - Staggered version TL/AD 
*
*object
*     see id section
*
*arguments
*     none
*
*implicits
#include "glb_ld.cdk"
#include "glb_pil.cdk"
#include "lun.cdk"
#include "schm.cdk"
#include "vth.cdk"
#include "vth_m.cdk"
#include "ind.cdk"
#include "v4dg.cdk"
#include "hzd.cdk"
#include "ind_m.cdk"
*
      integer i, j, k, key(10), pnerr
*
      integer  vmmlod, vmmget, vmmuld, nkey
      external vmmlod, vmmget, vmmuld
*
*     __________________________________________________________________
*
      if( Lun_out.gt.0 ) write(Lun_out,1003) 
*
!$omp parallel
*
      if(.not.Schm_hydro_L ) then
!$omp do
         do k=0,G_nk+1
            do j=1,l_nj
            do i=1,l_ni
*
*              TRAJECTORY
*              ----------
               Ind_m_q(i,j,k) = 0.
*
*              TLM
*              ---
               Ind_q(i,j,k) = 0.
*
            end do
            end do
         end do
!$omp enddo
      endif
*
!$omp end parallel
*
*     TRAJECTORY and TLM
*     ------------------
      call diag_fip_tl ( Ind_fip,  Ind_s,  Ind_t,  
     $                   Ind_m_fip,Ind_m_s,Ind_m_t,Ind_topo,
     $                   1,l_ni,1,l_nj,l_nk)
*
*     TRAJECTORY
*     ----------
      if (.not.(V4dg_4dvar_L.or.V4dg_sgvc_L)) call v4d_uv2img_tr
*
      if (Hzd_t1_0_L) call hzd_hoffld (Ind_m_u,   LDIST_DIM, G_nk, 1)
      if (Hzd_t1_0_L) call hzd_hoffld (Ind_m_v,   LDIST_DIM, G_nk, 2)
*
*     TLM
*     ---
      if (.not.(V4dg_4dvar_L.or.V4dg_sgvc_L)) call v4d_uv2img
*
      if (Hzd_t1_0_L) call hzd_hoffld (Ind_u,   LDIST_DIM, G_nk, 1)
      if (Hzd_t1_0_L) call hzd_hoffld (Ind_v,   LDIST_DIM, G_nk, 2)
*
      if(.FALSE.)then
*
      else
         do k= 1, G_nk+1
            do j = 1, l_nj
               do i = 1, l_ni
*
*                 TRAJECTORY
*                 ----------
                  Ind_m_zd(i,j,k)=0.
*
*                 TLM
*                 ---
                  Ind_zd(i,j,k)=0.
*
               end do
            end do
         end do
      endif
*
*     TRAJECTORY
*     ----------
      if (Hzd_t1_0_L) call hzd_hoffld (Ind_m_zd, LDIST_DIM, G_nk+1, 3)
*
*     TLM
*     ---
      if (Hzd_t1_0_L) call hzd_hoffld (Ind_zd, LDIST_DIM, G_nk+1, 3)
*
      key(1)=VMM_KEY(zdth)
      nkey=1
*
*     TRAJECTORY
*     ----------
      key(nkey+1)=VMM_KEY(zdth_m)
      nkey=nkey+1
*
      pnerr=vmmlod(key,nkey)
*
      pnerr=VMM_GET_VAR(zdth)
*
*     TRAJECTORY
*     ----------
      pnerr=VMM_GET_VAR(zdth_m)
*
!$omp parallel
!$omp do
      do k= 1, G_nk+1
         do j = 1, l_nj
            do i = 1, l_ni
*
*              TRAJECTORY
*              ----------
               Ind_m_w(i,j,k)=0.
*
*              TLM 
*              ---
               Ind_w(i,j,k)=0.
*
            end do
         end do
         if(k.le.G_nk) then
         do j = 1, l_nj
            do i = 1, l_ni
*
*              TRAJECTORY
*              ----------
               zdth_m(i,j,k)=0.
*
*              TLM
*              ----------
               zdth(i,j,k)=0.
*
            end do
         end do
         endif
      end do
!$omp enddo
!$omp end parallel
*
      pnerr=vmmuld(key,nkey)
*
1003  format(//,'TLM of PREPROCESSING DATA: (S/R V4D_PREDAT_TL PART 3)',
     %        /,'=====================================================',//)
*
*     __________________________________________________________________
      return
      end
