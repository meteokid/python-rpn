***s/r v4d_zero - Zero ALL model variables 
*
#include "model_macros_f.h"
*
      subroutine v4d_zero 
*
      implicit none
*
*author
*     M.Tanguay
*
*revision
* v2_10 - Tanguay M.        - initial MPI version
* v2_31 - Tanguay M.        - adapt ADJ for new advection code
*                           - adapt for tracers in tr3d  
* v3_03 - Tanguay M.        - Adjoint Lam and NoHyd configuration 
* v3_11 - Tanguay M.        - ADJ of digital filter 
* v3_21 - Lee V.            - removed Tr2d
* v3_30 - Tanguay M.        - Validation for LAM version 
*                           - adapt to bcs
* v4_04 - Tanguay M.        - Staggered version TL/AD 
*
*object
*    see id section
*
*arguments
*    none
*
*implicits
#include "lun.cdk"
#include "glb_ld.cdk"
#include "vt0.cdk"
#include "vt1.cdk"
#include "vth.cdk"
#include "vtx.cdk"
#include "rhsc.cdk"
#include "orh.cdk"
#include "tr3d.cdk"
#include "nest.cdk"
#include "schm.cdk"
#include "bcsdim.cdk"
#include "bcsmem.cdk"
*
*modules
      integer  vmmlod,vmmget,vmmuld
      external vmmlod,vmmget,vmmuld
      integer pnerr, pnlkey1(50), key1_, key0_, keyn_, 
     $        key1(Tr3d_ntr), key0(Tr3d_ntr), keyn(Tr3d_ntr), 
     $        pnlod, err, i, j, k, n, id 
*
      real tr1,tr0,trn
      pointer (patr1,  tr1(LDIST_SHAPE,*)),(patr0, tr0(LDIST_SHAPE,*))
      pointer (patrn,  trn(LDIST_SHAPE,*))
*
      if( Lun_debug_L ) then
         write(Lun_out,fmt='(/,''V4D_ZERO:Zero ALL model variables'')')
         write(Lun_out,fmt='(  ''================================='')')
      endif
*
*     ---------------------------
*     Zero comdeck vt0
*     ---------------------------
      pnlkey1( 1)= VMM_KEY(ut0  )
      pnlkey1( 2)= VMM_KEY(vt0  )
      pnlkey1( 3)= VMM_KEY(tt0  )
      pnlkey1( 4)= VMM_KEY(st0  )
      pnlkey1( 5)= VMM_KEY(zdt0 )
      pnlkey1( 6)= VMM_KEY(wt0  )
      pnlod =  6 
*
      if (.not. Schm_hydro_L) then
          pnlkey1(pnlod+1)= VMM_KEY(qt0 )
          pnlod = pnlod+1
      endif
*
      pnerr = vmmlod(pnlkey1,pnlod)
      pnerr = VMM_GET_VAR(ut0  )
      pnerr = VMM_GET_VAR(vt0  )
      pnerr = VMM_GET_VAR(tt0  )
      pnerr = VMM_GET_VAR(st0  )
      pnerr = VMM_GET_VAR(zdt0 )
      pnerr = VMM_GET_VAR(wt0  )
*
      if (.not. Schm_hydro_L) then
          pnerr = VMM_GET_VAR(qt0 )
      endif
*
      ut0  = 0.
      vt0  = 0.
      tt0  = 0.
      zdt0 = 0.
      st0  = 0.
      wt0  = 0.
*
      if (.not. Schm_hydro_L) then
         qt0 = 0.
      end if
*
      pnerr = vmmuld( -1, 0 )
*
      key0_ = VMM_KEY (trt0)
      do n=1,Tr3d_ntr
         key0(n) = key0_ + n
      end do
      if (Tr3d_ntr.gt.0) then
         err = vmmlod(key0,Tr3d_ntr)
         do n=1,Tr3d_ntr
            err = vmmget(key0(n),patr0,tr0)
            do k=1,l_nk+2
            do j=l_miny,l_maxy
            do i=l_minx,l_maxx
               tr0(i,j,k) = 0. 
            end do
            end do
            end do
         end do
         err = vmmuld(key0,Tr3d_ntr)
      endif
*
*     ---------------------------
*     Zero comdeck vt1
*     ---------------------------
      pnlkey1( 1)= VMM_KEY(ut1  )
      pnlkey1( 2)= VMM_KEY(vt1  )
      pnlkey1( 3)= VMM_KEY(tt1  )
      pnlkey1( 4)= VMM_KEY(st1  )
      pnlkey1( 5)= VMM_KEY(zdt1 )
      pnlkey1( 6)= VMM_KEY(fipt1)
      pnlkey1( 7)= VMM_KEY(wt1  )
*
      pnlkey1( 8)= VMM_KEY(xct1 )
      pnlkey1( 9)= VMM_KEY(yct1 )
      pnlkey1(10)= VMM_KEY(zct1 )
      pnlod = 10 
*
      if (.not. Schm_hydro_L) then
          pnlkey1(pnlod+1)= VMM_KEY(qt1 )
          pnlod = pnlod+1
      endif
*
      pnerr = vmmlod(pnlkey1,pnlod)
      pnerr = VMM_GET_VAR(ut1  )
      pnerr = VMM_GET_VAR(vt1  )
      pnerr = VMM_GET_VAR(tt1  )
      pnerr = VMM_GET_VAR(st1  )
      pnerr = VMM_GET_VAR(zdt1 )
      pnerr = VMM_GET_VAR(fipt1)
      pnerr = VMM_GET_VAR(wt1  )
*
      pnerr = VMM_GET_VAR(xct1 )
      pnerr = VMM_GET_VAR(yct1 )
      pnerr = VMM_GET_VAR(zct1 )
*
      if (.not. Schm_hydro_L) then
          pnerr = VMM_GET_VAR(qt1 )
      endif
*
      ut1  = 0.
      vt1  = 0.
      tt1  = 0.
      zdt1 = 0.
      fipt1= 0.
      st1  = 0.
      wt1  = 0.
*
      xct1 = 0.
      yct1 = 0.
      zct1 = 0.
*
      if (.not. Schm_hydro_L) then
         qt1 = 0.
      end if
*
      pnerr = vmmuld( -1, 0 )
*
      key1_ = VMM_KEY (trt1)
      do n=1,Tr3d_ntr
         key1(n) = key1_ + n
      end do
      if (Tr3d_ntr.gt.0) then
         err = vmmlod(key1,Tr3d_ntr)
         do n=1,Tr3d_ntr
            err = vmmget(key1(n),patr1,tr1)
            do k=1,l_nk+2
            do j=l_miny,l_maxy
            do i=l_minx,l_maxx
               tr1(i,j,k) = 0. 
            end do
            end do
            end do
         end do
         err = vmmuld(key1,Tr3d_ntr)
      endif
*
*     ---------------------------
*     Zero comdeck vth
*     ---------------------------
      pnlkey1(1) = VMM_KEY(uth  )
      pnlkey1(2) = VMM_KEY(vth  )
      pnlkey1(3) = VMM_KEY(zdth)
      pnlkey1(4) = VMM_KEY(xth  )
      pnlkey1(5) = VMM_KEY(yth  )
      pnlkey1(6) = VMM_KEY(zth  )
      pnlkey1(7) = VMM_KEY(xcth )
      pnlkey1(8) = VMM_KEY(ycth )
      pnlkey1(9) = VMM_KEY(zcth )
      pnlod = 9 
*
      pnerr = vmmlod(pnlkey1,pnlod)
      pnerr = VMM_GET_VAR(uth  )
      pnerr = VMM_GET_VAR(vth  )
      pnerr = VMM_GET_VAR(zdth)
      pnerr = VMM_GET_VAR(xth  )
      pnerr = VMM_GET_VAR(yth  )
      pnerr = VMM_GET_VAR(zth  )
      pnerr = VMM_GET_VAR(xcth )
      pnerr = VMM_GET_VAR(ycth )
      pnerr = VMM_GET_VAR(zcth )
*
      uth  = 0.
      vth  = 0.
      zdth = 0.
*
      xth  = 0.
      yth  = 0.
      zth  = 0.
      xcth = 0.
      ycth = 0.
      zcth = 0.
*
      pnerr = vmmuld( -1, 0 )
*
*     ---------------------------
*     Zero comdeck vtx
*     ---------------------------
      pnlkey1(1) = VMM_KEY(bspqtx)
      pnlkey1(2) = VMM_KEY(fiptx)
      pnlod = 2 
*
      if (.not. Schm_hydro_L) then
         pnlkey1(pnlod+1) = VMM_KEY(mutx)
         pnlod = pnlod+1
      endif
*
      pnerr = vmmlod(pnlkey1,pnlod)
      pnerr = VMM_GET_VAR(bspqtx)
      pnerr = VMM_GET_VAR(fiptx)
*
      if (.not. Schm_hydro_L) then
         pnerr = VMM_GET_VAR(mutx)
      endif
*
      bspqtx = 0.
      fiptx  = 0.
*
      if (.not. Schm_hydro_L) then
         mutx  = 0.
      endif
*
      pnerr = vmmuld( -1, 0 )
*
*     ---------------------------
*     Zero comdeck rhsc 
*     ---------------------------
      pnlkey1( 1)= VMM_KEY(rhsu )
      pnlkey1( 2)= VMM_KEY(rhsv )
      pnlkey1( 3)= VMM_KEY(rhst )
      pnlkey1( 4)= VMM_KEY(rhsc )
      pnlkey1( 5)= VMM_KEY(rhsw )
      pnlkey1( 6)= VMM_KEY(rhsf )
      pnlkey1( 7)= VMM_KEY(rhsp )
      pnlkey1( 8)= VMM_KEY(ruw1 )
      pnlkey1( 9)= VMM_KEY(rvw1 )
      pnlkey1(10)= VMM_KEY(ruw2 )
      pnlkey1(11)= VMM_KEY(rvw2 )
      pnlod = 11 
*
      pnerr = vmmlod(pnlkey1,pnlod)
      pnerr = VMM_GET_VAR(rhsu ) 
      pnerr = VMM_GET_VAR(rhsv )
      pnerr = VMM_GET_VAR(rhst )
      pnerr = VMM_GET_VAR(rhsc )
      pnerr = VMM_GET_VAR(rhsw )
      pnerr = VMM_GET_VAR(rhsf )
      pnerr = VMM_GET_VAR(rhsp )
      pnerr = VMM_GET_VAR(ruw1 )
      pnerr = VMM_GET_VAR(rvw1 )
      pnerr = VMM_GET_VAR(ruw2 )
      pnerr = VMM_GET_VAR(rvw2 )
*
      rhsu = 0.
      rhsv = 0.
      rhsc = 0.
      rhsp = 0.
      ruw1 = 0.
      rvw1 = 0.
      ruw2 = 0.
      rvw2 = 0.
*
      rhst = 0.
      rhsw = 0.
      rhsf = 0.
*
      pnerr = vmmuld( -1, 0 )
*
*     ---------------------------
*     Zero comdeck orh 
*     ---------------------------
      pnlkey1(1)= VMM_KEY(orhsu)
      pnlkey1(2)= VMM_KEY(orhsv)
      pnlkey1(3)= VMM_KEY(orhst)
      pnlkey1(4)= VMM_KEY(orhsc)
      pnlkey1(5)= VMM_KEY(orhsw)
      pnlkey1(6)= VMM_KEY(orhsf)
      pnlod = 6 
*
      pnerr = vmmlod(pnlkey1,pnlod)
      pnerr = VMM_GET_VAR(orhsu)
      pnerr = VMM_GET_VAR(orhsv)
      pnerr = VMM_GET_VAR(orhst)
      pnerr = VMM_GET_VAR(orhsc)
      pnerr = VMM_GET_VAR(orhsw)
      pnerr = VMM_GET_VAR(orhsf)
*
      orhsu = 0.
      orhsv = 0.
      orhsc = 0.
*
      orhst = 0.
      orhsw = 0.
      orhsf = 0.
*
      pnerr = vmmuld( -1, 0 )
*
      if(G_lam) then
*
*     ---------------------------
*     Zero comdeck nest 
*     ---------------------------
      pnlkey1( 1)=VMM_KEY(nest_u  )
      pnlkey1( 2)=VMM_KEY(nest_v  )
      pnlkey1( 3)=VMM_KEY(nest_w  )
      pnlkey1( 4)=VMM_KEY(nest_t  )
      pnlkey1( 5)=VMM_KEY(nest_zd )
      pnlkey1( 6)=VMM_KEY(nest_s  )
      pnlod =  6   
*
      if (.not. Schm_hydro_L) then
         pnlkey1(pnlod+1)=VMM_KEY(nest_q )
         pnlod = pnlod+1
      endif
*
      err = vmmlod(pnlkey1,pnlod)
      err = VMM_GET_VAR(nest_u  )
      err = VMM_GET_VAR(nest_v  )
      err = VMM_GET_VAR(nest_w  )
      err = VMM_GET_VAR(nest_t  )
      err = VMM_GET_VAR(nest_zd )
      err = VMM_GET_VAR(nest_s  )
*
      if (.not. Schm_hydro_L) then
         err = VMM_GET_VAR(nest_q )
      endif
*
      nest_u  = 0. 
      nest_v  = 0. 
      nest_w  = 0.
      nest_t  = 0. 
      nest_zd = 0. 
      nest_s  = 0. 
*
      if (.not. Schm_hydro_L) nest_q = 0. 
*
      pnerr = vmmuld( -1, 0 )
*
      keyn_ = VMM_KEY (nest_tr)
      do n=1,Tr3d_ntr
         keyn(n) = keyn_ + n
      end do
      if (Tr3d_ntr.gt.0) then
         err = vmmlod(keyn,Tr3d_ntr)
         do n=1,Tr3d_ntr
            err = vmmget(keyn(n),patrn,trn)
            do k=1,l_nk+2
            do j=l_miny,l_maxy
            do i=l_minx,l_maxx
               trn(i,j,k) = 0. 
            end do 
            end do
            end do
         end do
         err = vmmuld(keyn,Tr3d_ntr)
      endif
*
      endif
*
      if(G_lam) then
*
*     ----------------
*     Zero comdeck bcs
*     ----------------
      do i=1,bcs_sz
         bcs_u  (i) = 0.
         bcs_v  (i) = 0.
         bcs_w  (i) = 0.
         bcs_t  (i) = 0.
         bcs_zd (i) = 0.
      end do
*
      if (.not. Schm_hydro_L) then
      do i=1,bcs_sz
         bcs_q  (i) = 0.
      end do
      end if
*
      do i=1,bcs_sz
         bcs_s(i) = 0.
      end do
*
      if (Tr3d_ntr.gt.0) then
         do n=1,Tr3d_ntr
            id = (n-1)*bcs_sz+1
*
            do i=1,bcs_sz
               bcs_tr (id+i-1) = 0.
            end do
*
         end do
      endif
*
      endif
*
      return
      end
