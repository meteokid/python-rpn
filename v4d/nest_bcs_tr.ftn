***s/r nest_bcs_tr - Equivalent to nest_bcs for TRAJECTORY 
*
#include "model_macros_f.h"
*
      subroutine nest_bcs_tr ()
      implicit none
*author 
*
*revision
*
*object
*
*arguments
*       none
*
*implicits
#include "glb_ld.cdk"
#include "bcsdim.cdk"
#include "bcsgrds.cdk"
C#include "bcsmem.cdk"
#include "vt1m.cdk"
#include "nestm.cdk"
#include "tr3d.cdk"
#include "lam.cdk"
#include "schm.cdk"
*
      integer  vmmlod,vmmget,vmmuld
      external vmmlod,vmmget,vmmuld
*
      integer err,key(26),i,j,k,nvar,id
      integer key0(Tr3d_ntr),key0_,key1(Tr3d_ntr),key1_, n
      real trm,tr0m
      pointer (patrm, trm(LDIST_SHAPE,*)),(patr0m,tr0m(LDIST_SHAPE,*))
*----------------------------------------------------------------------
*
      if (north+south+west+east.lt.1) then
*
         if (Lam_toptt_L) then
*
*           Pilot the temperature for the whole top level
            key(1 )= VMM_KEY(nestm_tm)
            key(2 )= VMM_KEY(tt1m)
            nvar   = 2
            err = VMM_GET_VAR(nestm_tm)
            err = VMM_GET_VAR(tt1m)
*
            do j=1,l_nj
               do i=1,l_ni
                  tt1m(i,j,1) = nestm_tm(i,j,1)
               end do
            end do
*
            err = vmmuld(key,nvar)
*
         endif
*
         return
      endif
*
      key(1 )=VMM_KEY(nestm_um)
      key(2 )=VMM_KEY(nestm_vm)
      key(3 )=VMM_KEY(nestm_tm)
      key(4 )=VMM_KEY(nestm_psdm)
      key(5 )=VMM_KEY(nestm_pipm)
      key(6 )=VMM_KEY(nestm_fipm)
      key(7 )=VMM_KEY(nestm_tdm)
      key(8 )=VMM_KEY(nestm_fim)
      key(9 )=VMM_KEY(nestm_qm)
      key(10)=VMM_KEY(nestm_sm)
      key(11)=VMM_KEY(nestm_tpm)
      key(12)=VMM_KEY(ut1m)
      key(13)=VMM_KEY(vt1m)
      key(14)=VMM_KEY(tt1m)
      key(15)=VMM_KEY(psdt1m)
      key(16)=VMM_KEY(pipt1m)
      key(17)=VMM_KEY(fipt1m)
      key(18)=VMM_KEY(tdt1m)
      key(19)=VMM_KEY(fit1m)
      key(20)=VMM_KEY(qt1m)
      key(21)=VMM_KEY(st1m)
      key(22)=VMM_KEY(tpt1m)
      nvar = 22
*
      if (.not.Schm_hydro_L) then
         key(23)=VMM_KEY(nestm_wm)
         key(24)=VMM_KEY(nestm_mum)
         key(25)=VMM_KEY(wt1m)
         key(26)=VMM_KEY(mut1m)
         nvar = 26
      endif
*
      err = vmmlod(key,nvar)
      err = VMM_GET_VAR(nestm_um)
      err = VMM_GET_VAR(nestm_vm)
      err = VMM_GET_VAR(nestm_tm)
      err = VMM_GET_VAR(nestm_psdm)
      err = VMM_GET_VAR(nestm_pipm)
      err = VMM_GET_VAR(nestm_fipm)
      err = VMM_GET_VAR(nestm_tdm)
      err = VMM_GET_VAR(nestm_fim)
      err = VMM_GET_VAR(nestm_qm)
      err = VMM_GET_VAR(nestm_sm)
      err = VMM_GET_VAR(nestm_tpm)
      err = VMM_GET_VAR(ut1m)
      err = VMM_GET_VAR(vt1m)
      err = VMM_GET_VAR(tt1m)
      err = VMM_GET_VAR(psdt1m)
      err = VMM_GET_VAR(pipt1m)
      err = VMM_GET_VAR(fipt1m)
      err = VMM_GET_VAR(tdt1m)
      err = VMM_GET_VAR(fit1m)
      err = VMM_GET_VAR(qt1m)
      err = VMM_GET_VAR(st1m)
      err = VMM_GET_VAR(tpt1m)
      if (.not.Schm_hydro_L) then
         err = VMM_GET_VAR(nestm_wm)
         err = VMM_GET_VAR(nestm_mum)
         err = VMM_GET_VAR(wt1m)
         err = VMM_GET_VAR(mut1m)
      endif
*
      if (l_north) then
         do k=1,G_nk
            do j=l_nj-pil_n+1,l_nj
            do i=1,l_ni
               tt1m  (i,j,k) = nestm_tm  (i,j,k)
               psdt1m(i,j,k) = nestm_psdm(i,j,k)
               pipt1m(i,j,k) = nestm_pipm(i,j,k)
               fipt1m(i,j,k) = nestm_fipm(i,j,k)
               tdt1m (i,j,k) = nestm_tdm (i,j,k)
               fit1m (i,j,k) = nestm_fim (i,j,k)
               qt1m  (i,j,k) = nestm_qm  (i,j,k)            
               tpt1m (i,j,k) = nestm_tpm (i,j,k)            
            end do
            end do
            do j=l_nj-pil_n+1,l_nj
            do i=1,l_niu
               ut1m  (i,j,k) = nestm_um (i,j,k)
            end do
            end do
            do j=l_nj-pil_n,l_njv
            do i=1,l_ni
               vt1m  (i,j,k) = nestm_vm (i,j,k)
            end do
            end do
         end do
         do j=l_nj-pil_n+1,l_nj
         do i=1,l_ni
            st1m  (i,j) = nestm_sm (i,j)
         end do
         end do
         if (.not. Schm_hydro_L) then
            do k=1,G_nk
            do j=l_nj-pil_n+1,l_nj
            do i=1,l_ni
               wt1m (i,j,k) = nestm_wm (i,j,k)
               mut1m(i,j,k) = nestm_mum(i,j,k)
            end do
            end do
            end do
         endif
      endif
*
      if (l_east) then
         do k=1,G_nk
            do j=1,l_nj
            do i=l_ni-pil_e+1,l_ni
               tt1m  (i,j,k) = nestm_tm  (i,j,k)
               psdt1m(i,j,k) = nestm_psdm(i,j,k)
               pipt1m(i,j,k) = nestm_pipm(i,j,k)
               fipt1m(i,j,k) = nestm_fipm(i,j,k)
               tdt1m (i,j,k) = nestm_tdm (i,j,k)
               fit1m (i,j,k) = nestm_fim (i,j,k)
               qt1m  (i,j,k) = nestm_qm  (i,j,k)            
               tpt1m (i,j,k) = nestm_tpm (i,j,k)            
            end do
            end do
            do j=1,l_nj
            do i=l_ni-pil_e,l_niu
               ut1m  (i,j,k) = nestm_um  (i,j,k)
            end do
            end do
            do j=1,l_njv
            do i=l_ni-pil_e+1,l_ni
               vt1m  (i,j,k) = nestm_vm  (i,j,k)
            end do
            end do
         end do
         do j=1,l_nj
         do i=l_ni-pil_e+1,l_ni
            st1m  (i,j) = nestm_sm  (i,j)
         end do
         end do
         if (.not. Schm_hydro_L) then
            do k=1,G_nk
            do j=1,l_nj
            do i=l_ni-pil_e+1,l_ni
               wt1m (i,j,k) = nestm_wm (i,j,k)
               mut1m(i,j,k) = nestm_mum(i,j,k)
            end do
            end do
            end do
         endif
      endif
*
      if (l_south) then
         do k=1,G_nk
            do j=1,pil_s
            do i=1,l_ni
               vt1m  (i,j,k) = nestm_vm  (i,j,k)
               tt1m  (i,j,k) = nestm_tm  (i,j,k)
               psdt1m(i,j,k) = nestm_psdm(i,j,k)
               pipt1m(i,j,k) = nestm_pipm(i,j,k)
               fipt1m(i,j,k) = nestm_fipm(i,j,k)
               tdt1m (i,j,k) = nestm_tdm (i,j,k)
               fit1m (i,j,k) = nestm_fim (i,j,k)
               qt1m  (i,j,k) = nestm_qm  (i,j,k)            
               tpt1m (i,j,k) = nestm_tpm (i,j,k)            
            end do
            end do
            do j=1,pil_s
            do i=1,l_niu
               ut1m  (i,j,k) = nestm_um  (i,j,k)
            end do
            end do
         end do
         do j=1,pil_s
         do i=1,l_ni
            st1m  (i,j) = nestm_sm  (i,j)
         end do
         end do
         if (.not. Schm_hydro_L) then
            do k=1,G_nk
            do j=1,pil_s
            do i=1,l_ni
               wt1m (i,j,k) = nestm_wm (i,j,k)
               mut1m(i,j,k) = nestm_mum(i,j,k)
            end do
            end do
            end do
         endif
      endif
*
      if (l_west) then
         do k=1,G_nk
            do j=1,l_nj
            do i=1,pil_w
               ut1m  (i,j,k) = nestm_um  (i,j,k)
               tt1m  (i,j,k) = nestm_tm  (i,j,k)
               psdt1m(i,j,k) = nestm_psdm(i,j,k)
               pipt1m(i,j,k) = nestm_pipm(i,j,k)
               fipt1m(i,j,k) = nestm_fipm(i,j,k)
               tdt1m (i,j,k) = nestm_tdm (i,j,k)
               fit1m (i,j,k) = nestm_fim (i,j,k)
               qt1m  (i,j,k) = nestm_qm  (i,j,k)            
               tpt1m (i,j,k) = nestm_tpm (i,j,k)            
            end do
            end do
            do j=1,l_njv
            do i=1,pil_w
               vt1m  (i,j,k) = nestm_vm  (i,j,k)
            end do
            end do
         end do
         do j=1,l_nj
         do i=1,pil_w
            st1m  (i,j) = nestm_sm  (i,j)
         end do
         end do
         if (.not. Schm_hydro_L) then
            do k=1,G_nk
            do j=1,l_nj
            do i=1,pil_w
               wt1m (i,j,k) = nestm_wm (i,j,k)
               mut1m(i,j,k) = nestm_mum(i,j,k)
            end do
            end do
            end do
         endif
      endif
*
      if (Lam_toptt_L) then
*
*        Pilot the temperature for the whole top level
         do j=1,l_nj
            do i=1,l_ni
               tt1m(i,j,1) = nestm_tm(i,j,1)
            end do
         end do
      endif
*
      err = vmmuld(key,nvar)
*
      key1_ = VMM_KEY (nestm_trm)
      key0_ = VMM_KEY (trt1m)
      do n=1,Tr3d_ntr
         key1(n) = key1_ + n
         key0(n) = key0_ + n
      end do
      if (Tr3d_ntr.gt.0) then
         err = vmmlod(key1,Tr3d_ntr)
         err = vmmlod(key0,Tr3d_ntr)
         do n=1,Tr3d_ntr
            id = (n-1)*bcs_sz+1
            err = vmmget(key1(n),patrm ,trm )
            err = vmmget(key0(n),patr0m,tr0m)
            if (l_north) then
                do k=1,G_nk
                   do j=l_nj-pil_n+1,l_nj
                   do i=1,l_ni
                      tr0m  (i,j,k) = trm  (i,j,k)
                   enddo
                   enddo
                enddo
            endif
            if (l_east) then
                do k=1,G_nk
                   do j=1,l_nj
                   do i=l_ni-pil_e+1,l_ni
                      tr0m  (i,j,k) = trm  (i,j,k)
                   enddo
                   enddo
                enddo
            endif
            if (l_south) then
                do k=1,G_nk
                   do j=1,pil_s
                   do i=1,l_ni
                      tr0m  (i,j,k) = trm  (i,j,k)
                   enddo
                   enddo
                enddo
            endif
            if (l_west) then
                do k=1,G_nk
                   do j=1,l_nj
                   do i=1,pil_w
                      tr0m  (i,j,k) = trm  (i,j,k)
                   enddo
                   enddo
                enddo
            endif
         enddo
         err = vmmuld(key1,Tr3d_ntr)
         err = vmmuld(key0,Tr3d_ntr)
      endif
*
*----------------------------------------------------------------------
      return
 1000 format (/,19('#'),' NEST_BCS STAT TR ',i6,1X,19('#'))
      end
