***s/r vspng_main - TLM of vspng_main 
*
#include "model_macros_f.h"
*
      subroutine vspng_main_tl
*
      implicit none
*
*author
*     M.Tanguay
*
*revision
* v2_21 - Tanguay M.        - initial MPI version
* v2_30 - Edouard S.        - replace Schm_elast_L by Schm_cptop_L
* v2_31 - Tanguay M.        - adapt for vertical hybrid coordinate 
*                           - change name 
* v3_03 - Tanguay M.        - Adjoint Lam and NoHyd configuration
* v3_11 - Tanguay M.        - AIXport+Opti+OpenMP for TLM-ADJ
* v3_20 - Tanguay M.        - Option of storing instead of redoing TRAJ
* v4_04 - Tanguay M.        - Staggered version TL/AD 
*
*object
*     see id section
*	
*arguments
*     none
*
*implicits
#include "glb_ld.cdk"
#include "lun.cdk"
#include "schm.cdk"
#include "vspng.cdk"
#include "vt1.cdk"
#include "vt1_m.cdk"
*
*modules
      integer  vmmlod,vmmuld,vmmget
      external vmmlod,vmmuld,vmmget
*
      integer pnerr, pnlod, pnlkey1(10)
*
*     ______________________________________________________
*
      if (Vspng_nk.lt.1) return
*     ______________________________________________________
*
      pnlkey1(1) = VMM_KEY(ut1)
      pnlkey1(2) = VMM_KEY(vt1)
      pnlkey1(3) = VMM_KEY(zdt1)
      pnlkey1(4) = VMM_KEY(tt1)
      pnlod = 4
      if (.not. Schm_hydro_L) then
         pnlod = pnlod +1
         pnlkey1(pnlod) = VMM_KEY(wt1)
      endif
*
*     TRAJECTORY
*     ----------
      pnlkey1(pnlod+1) = VMM_KEY(ut1_m)
      pnlkey1(pnlod+2) = VMM_KEY(vt1_m)
      pnlkey1(pnlod+3) = VMM_KEY(zdt1_m)
      pnlkey1(pnlod+4) = VMM_KEY(tt1_m)
      pnlod = pnlod+4
      if (.not. Schm_hydro_L) then
         pnlod = pnlod +1
         pnlkey1(pnlod) = VMM_KEY(wt1_m)
      endif
*
*     - - - - - - - - - - - - - - -
      pnerr = vmmlod(pnlkey1,pnlod)
*     - - - - - - - - - - - - - - -
      pnerr = VMM_GET_VAR(ut1)
      pnerr = VMM_GET_VAR(vt1)
      pnerr = VMM_GET_VAR(zdt1)
      pnerr = VMM_GET_VAR(tt1)
      if (.not. Schm_hydro_L) then
         pnerr = VMM_GET_VAR(wt1)
      endif
*
*     TRAJECTORY
*     ----------
      pnerr = VMM_GET_VAR(ut1_m)
      pnerr = VMM_GET_VAR(vt1_m)
      pnerr = VMM_GET_VAR(zdt1_m)
      pnerr = VMM_GET_VAR(tt1_m)
      if (.not. Schm_hydro_L) then
         pnerr = VMM_GET_VAR(wt1_m)
      endif
*
      call iw2rwnd ( ut1_m, vt1_m, LDIST_DIM, G_nk, 1 )
      call iw2rwnd ( ut1,   vt1,   LDIST_DIM, G_nk, 1 )
*
      if (G_lam) then
         if (Lun_debug_L) write (Lun_out,1001) Vspng_nk
         call vspng_drv_lam_tl (ut1,  vt1,  zdt1,  wt1,  tt1,
     %                          ut1_m,vt1_m,zdt1_m,wt1_m,tt1_m,LDIST_DIM,G_nk)
      else
         if (Lun_debug_L) write (Lun_out,1000) Vspng_nk
         call vspng_drv_tl (ut1,  vt1,  zdt1,  wt1,  tt1,
     %                      ut1_m,vt1_m,zdt1_m,wt1_m,tt1_m,LDIST_DIM,G_nk)
      endif
*
      call iw2rwnd ( ut1_m,vt1_m,LDIST_DIM, G_nk, 2 )
      call iw2rwnd ( ut1,  vt1,  LDIST_DIM, G_nk, 2 )
*
      pnerr = vmmuld(-1,0)
*
 1000 format(/,3X,
     $ 'TLM of DEL-2 SPONGE LAYER ON TOP ',I3,' LAYERS: (S/R VSPNG_DRV_TL)')
 1001 format(/,3X,
     $ 'TLM of 9PT FILT SPONGE LAYER ON TOP ',I3,' LAYERS: (S/R VSPNG_DRV_LAM_TL)')
*
      return
      end
