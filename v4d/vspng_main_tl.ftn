***s/r vspng_main - TLM of vspng_main 
*
#include "model_macros_f.h"
*
      subroutine vspng_main_tl
*
      implicit none
*
*author
*     M.Tanguay
*
*revision
* v2_21 - Tanguay M.        - initial MPI version
* v2_30 - Edouard S.        - replace Schm_elast_L by Schm_cptop_L
* v2_31 - Tanguay M.        - adapt for vertical hybrid coordinate 
*                           - change name 
* v3_03 - Tanguay M.        - Adjoint Lam and NoHyd configuration
* v3_11 - Tanguay M.        - AIXport+Opti+OpenMP for TLM-ADJ
* v3_20 - Tanguay M.        - Option of storing instead of redoing TRAJ
* v4_04 - Tanguay M.        - Staggered version TL/AD 
* v4_10 - Tanguay M.        - VMM replacement with GMM for (TL/AD)
*
*object
*     see id section
*	
*implicits
#include "gmm.hf"
#include "var_gmm.cdk"
#include "glb_ld.cdk"
#include "lun.cdk"
#include "schm.cdk"
#include "vspng.cdk"
#include "vt1.cdk"
#include "vt1_m.cdk"
*
*     ______________________________________________________
*
      if (Vspng_nk.lt.1) return
*     ______________________________________________________
*
      gmmstat = gmm_get(gmmk_ut1_s,ut1,meta3d)
      if (GMM_IS_ERROR(gmmstat)) print *,'vspng_main_tl ERROR at gmm_get(ut1)'
      gmmstat = gmm_get(gmmk_vt1_s,vt1,meta3d)
      if (GMM_IS_ERROR(gmmstat)) print *,'vspng_main_tl ERROR at gmm_get(vt1)'
      gmmstat = gmm_get(gmmk_zdt1_s,zdt1,meta3d)
      if (GMM_IS_ERROR(gmmstat)) print *,'vspng_main_tl ERROR at gmm_get(zdt1)'
      gmmstat = gmm_get(gmmk_tt1_s,tt1,meta3d)
      if (GMM_IS_ERROR(gmmstat)) print *,'vspng_main_tl ERROR at gmm_get(tt1)'
      gmmstat = gmm_get(gmmk_wt1_s,wt1,meta3d)
      if (GMM_IS_ERROR(gmmstat)) print *,'vspng_main_tl ERROR at gmm_get(wt1)'
*
*     TRAJECTORY
*     ----------
      gmmstat = gmm_get(gmmk_ut1_m_s,ut1_m,meta3d)
      if (GMM_IS_ERROR(gmmstat)) print *,'vspng_main_tl ERROR at gmm_get(ut1_m)'
      gmmstat = gmm_get(gmmk_vt1_m_s,vt1_m,meta3d)
      if (GMM_IS_ERROR(gmmstat)) print *,'vspng_main_tl ERROR at gmm_get(vt1_m)'
      gmmstat = gmm_get(gmmk_zdt1_m_s,zdt1_m,meta3d)
      if (GMM_IS_ERROR(gmmstat)) print *,'vspng_main_tl ERROR at gmm_get(zdt1_m)'
      gmmstat = gmm_get(gmmk_tt1_m_s,tt1_m,meta3d)
      if (GMM_IS_ERROR(gmmstat)) print *,'vspng_main_tl ERROR at gmm_get(tt1_m)'
      gmmstat = gmm_get(gmmk_wt1_m_s,wt1_m,meta3d)
      if (GMM_IS_ERROR(gmmstat)) print *,'vspng_main_tl ERROR at gmm_get(wt1_m)'
*
      call iw2rwnd ( ut1_m, vt1_m, LDIST_DIM, G_nk, 1 )
      call iw2rwnd ( ut1,   vt1,   LDIST_DIM, G_nk, 1 )
*
      if (G_lam) then
         if (Lun_debug_L) write (Lun_out,1001) Vspng_nk
         call vspng_drv_lam_tl (ut1,  vt1,  zdt1,  wt1,  tt1,
     %                          ut1_m,vt1_m,zdt1_m,wt1_m,tt1_m,LDIST_DIM,G_nk)
      else
         if (Lun_debug_L) write (Lun_out,1000) Vspng_nk
         call vspng_drv_tl (ut1,  vt1,  zdt1,  wt1,  tt1,
     %                      ut1_m,vt1_m,zdt1_m,wt1_m,tt1_m,LDIST_DIM,G_nk)
      endif
*
      call iw2rwnd ( ut1_m,vt1_m,LDIST_DIM, G_nk, 2 )
      call iw2rwnd ( ut1,  vt1,  LDIST_DIM, G_nk, 2 )
*
 1000 format(/,3X,
     $ 'TLM of DEL-2 SPONGE LAYER ON TOP ',I3,' LAYERS: (S/R VSPNG_DRV_TL)')
 1001 format(/,3X,
     $ 'TLM of 9PT FILT SPONGE LAYER ON TOP ',I3,' LAYERS: (S/R VSPNG_DRV_LAM_TL)')
*
      return
      end
