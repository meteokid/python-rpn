***s/r pre_tr - Equivalent to pre for TRAJECTORY   
*
#include "model_macros_f.h"
*
      subroutine pre_tr
*
      implicit none
*
*author
*     M.Tanguay
*
*revision
* v2_10 - Tanguay M.        - initial MPI version
* v2_21 - Tanguay M.        - reduce standard output as in model 
* v2_31 - Tanguay M.        - adapt ADJ for new advection code 
* v3_03 - Tanguay M.        - Adjoint NoHyd configuration 
* v3_11 - Tanguay M.        - AIXport+Opti+OpenMP for TLM-ADJ
* v4_04 - Tanguay M.        - Staggered version TL/AD 
*
*object
*     see id section
*
*arguments
*     none
*
*implicits
#include "glb_ld.cdk"
#include "lun.cdk"
#include "geomg.cdk"
#include "rhsc_m.cdk"
#include "orh_m.cdk"
#include "vt1_m.cdk"
#include "p_geof.cdk"
#include "schm.cdk"
#include "lctl.cdk"
*
*modules
      integer  vmmlod, vmmget, vmmuld
      external vmmlod, vmmget, vmmuld
*
      integer pnerr, pnlod, pnlkey1(19), i, j 
*     ______________________________________________________
*
*
      if (Lun_debug_L) write (Lun_out,1000)
*
      pnlkey1(1)  = VMM_KEY(rhsu_m)
      pnlkey1(2)  = VMM_KEY(rhsv_m)
      pnlkey1(3)  = VMM_KEY(ruw1_m)
      pnlkey1(4)  = VMM_KEY(ruw2_m)
      pnlkey1(5)  = VMM_KEY(rvw1_m)
      pnlkey1(6)  = VMM_KEY(rvw2_m)
      pnlkey1(7)  = VMM_KEY(xct1_m)
      pnlkey1(8)  = VMM_KEY(yct1_m)
      pnlkey1(9)  = VMM_KEY(zct1_m)
      pnlkey1(10) = VMM_KEY(topo)
      pnlkey1(11) = VMM_KEY(rhsc_m)
      pnlkey1(12) = VMM_KEY(rhst_m)
      pnlkey1(13) = VMM_KEY(orhsu_m)
      pnlkey1(14) = VMM_KEY(orhsv_m)
      pnlkey1(15) = VMM_KEY(rhsf_m)
      pnlod = 15
      if (.not. Schm_hydro_L) then
         pnlkey1(16) = VMM_KEY(rhsw_m)
         pnlod = 16
      endif
*     - - - - - - - - - - - - - - -
      pnerr = vmmlod(pnlkey1,pnlod)
*     - - - - - - - - - - - - - - -
      pnerr = VMM_GET_VAR(rhsu_m)
      pnerr = VMM_GET_VAR(rhsv_m)
      pnerr = VMM_GET_VAR(ruw1_m)
      pnerr = VMM_GET_VAR(ruw2_m)
      pnerr = VMM_GET_VAR(rvw1_m)
      pnerr = VMM_GET_VAR(rvw2_m)
      pnerr = VMM_GET_VAR(xct1_m)
      pnerr = VMM_GET_VAR(yct1_m)
      pnerr = VMM_GET_VAR(zct1_m)
      pnerr = VMM_GET_VAR(topo)
      pnerr = VMM_GET_VAR(rhsc_m)
      pnerr = VMM_GET_VAR(rhst_m)
      pnerr = VMM_GET_VAR(orhsu_m)
      pnerr = VMM_GET_VAR(orhsv_m)
      pnerr = VMM_GET_VAR(rhsf_m)
      if (.not. Schm_hydro_L) then
         pnerr = VMM_GET_VAR(rhsw_m)
      else
         rhsw_m_ = 0
      endif
*
      call prep_2 ( rhsu_m ,rhsv_m ,ruw1_m  ,ruw2_m ,rvw1_m ,rvw2_m ,
     %              xct1_m ,yct1_m ,zct1_m  ,topo   ,rhsc_m ,rhst_m ,
     %              rhsw_m ,rhsf_m ,orhsu_m ,orhsv_m,
     %              LDIST_DIM, l_ni, l_nj, l_nk )
*
      pnerr = vmmuld(-1,0)
*
1000  format(3X,'TRAJ of PRE-COMPUTATION FOR THE HELMHOLTZ PROBLEM: (S/R PRE_TR)',/)

      return
      end
