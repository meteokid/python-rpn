prgemnml: prgemnml.o
	$(RBUILD) -bidon -main prgemnml -obj $(LCLPO)/prgemnml.o -o $(ATM_MODEL)prnml_$(BASE_ARCH)_${ATM_MODEL_VERSION}.Abs -libpath $(LIBPATH) -libappl "gemdyn" -librmn $(RMN_VERSION)
	/bin/rm -f $(LCLPO)/prgemnml.o

gemgrid:
	$(RBUILD) -bidon -main gemgrid -o $(ATM_MODEL)grid_$(BASE_ARCH)_${ATM_MODEL_VERSION}.Abs -libpath $(LIBPATH) -libappl "gemdyn $(COMM)" -librmn $(RMN_VERSION)

MONBINDIR=.
monitor:
	$(MAKE) gem_monitor_end
	$(MAKE) gem_monitor_output
	mv gem_monitor_end gem_monitor_output $(MONBINDIR)

allbin: prgemnml gem
	$(MAKE) gemgrid COMM_stubs=rpn_commstubs$(COMM_VERSION)
#	$(MAKE) gem
#	$(MAKE) gem V4D="v4d modulopt prof_003"
