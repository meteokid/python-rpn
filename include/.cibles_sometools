
modelnml  = $(MODEL)prnml_$(BASE_ARCH)_${MODEL_VERSION}.Abs
prgemnml: prgemnml.o
	$(RBUILD) -bidon -main prgemnml -obj $(LCLPO)/prgemnml.o -libpath $(LIBPATH) -arch $(ARCH) -abi $(ABI) -optf "=$(LFLAGS)" -o $(modelnml)  -libappl "$(LIBS)" -librmn $(RMN) -libpriv $(PRIV) -codebeta $(CODEBETA) rpn_comm_stubs
	/bin/rm -f $(LCLPO)/prgemnml.o

phynml    = phyprnml_$(BASE_ARCH)_${MODEL_VERSION}.Abs
prphynml: prphynml.o
	$(RBUILD) -bidon -main prphynml -obj $(LCLPO)/prphynml.o -libpath $(LIBPATH) -arch $(ARCH) -abi $(ABI) -optf "=$(LFLAGS)" -o $(phynml)  -libappl "$(LIBS)" -librmn $(RMN) -libpriv $(PRIV) -codebeta $(CODEBETA) rpn_comm_stubs
	/bin/rm -f $(LCLPO)/prphynml.o

modelgrid = $(MODEL)grid_$(BASE_ARCH)_${MODEL_VERSION}.Abs
gemgrid: gemgrid.o
	$(RBUILD) -bidon -main gemgrid -obj $(LCLPO)/gemgrid.o -libpath $(LIBPATH) -arch $(ARCH) -abi $(ABI) -optf "=$(LFLAGS)" -o $(modelgrid)  -libappl "$(LIBS)" -librmn $(RMN) -libpriv $(PRIV) -codebeta $(CODEBETA) rpn_comm_stubs
	/bin/rm -f $(LCLPO)/gemgrid.o

allbin: gemgrid
	make gem V4D="v4d modulopt prof_003" CPL="cpl"
