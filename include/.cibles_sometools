prgemnml: prgemnml.o
	$(RBUILD) -bidon -main prgemnml -obj $(LCLPO)/prgemnml.o -o $(ATM_MODEL)prnml_$(BASE_ARCH)_${ATM_MODEL_VERSION}.Abs
	/bin/rm -f $(LCLPO)/prgemnml.o

gemgrid:
	$(RBUILD) -bidon -main gemgrid -o $(ATM_MODEL)grid_$(BASE_ARCH)_${ATM_MODEL_VERSION}.Abs -libpath $(LIBPATH) -libappl "gem rpn_comm222" -librmn $(RMN) -codebeta rpn_comm_stubs

#allbin: gemgrid
#	make gem V4D="v4d modulopt prof_003" CPL="cpl_105"
allbin: prgemnml gemgrid
	make gem 


